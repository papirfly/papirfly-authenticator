{
    "name": "@papirfly/papirfly-authenticator",
    "version": "0.0.7",
    "license": "MIT",
    "homepage": "https://github.com/papirfly/papirfly-authenticator",
    "repository": {
        "type": "git",
        "url": "https://github.com/papirfly/papirfly-authenticator.git"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "clean": "rimraf dist/*",
        "lint": "eslint -c .eslintrc.json --fix --ext .ts src",
        "tsc": "tsc",
        "watch": "tsc --watch",
        "preversion": "yarn lint",
        "version": "yarn build && git add -A",
        "postversion": "git push && git push --tags",
        "test": "karma start karma.conf.js",
        "prepublishOnly": "yarn build",
        "build": "yarn clean && yarn tsc -p ./src",
        "publish_all": "yarn publish_npm && yarn publish_internal",
        "publish_npm": "yarn publish --registry=https://registry.npmjs.org --access public",
        "publish_internal": "yarn publish --registry=https://npm.pf.papirfly.no/"
    },
    "dependencies": {
        "typescript": "^4.8.2"
    },
    "devDependencies": {
        "@types/jasmine": "^4.3.0",
        "@types/sinon": "^10.0.13",
        "@typescript-eslint/eslint-plugin": "5.38.1",
        "@typescript-eslint/parser": "5.38.1",
        "eslint": "8.24.0",
        "eslint-plugin-jasmine": "4.1.3",
        "jasmine-core": "^4.5.0",
        "karma": "^6.4.1",
        "karma-chrome-launcher": "^3.1.1",
        "karma-jasmine": "^5.1.0",
        "karma-typescript": "^5.5.3",
        "prettier": "^2.7.1",
        "prettier-plugin-organize-imports": "3.1.1",
        "puppeteer": "^19.2.2",
        "rimraf": "^3.0.2",
        "sinon": "^14.0.2"
    }
}